{
  "name": "oxid-projects/paypal-apiv2",
  "type": "project",
  "description": "PayPal API V2",
  "license": [
    "GPL-3.0-only",
    "proprietary"
  ],
  "minimum-stability": "RC",
  "repositories": [
    {
      "type": "path",
      "url": "custom/*"
    },
    {
      "type": "composer",
      "url": "https://b2b-edition.packages.oxid-esales.com"
    },
    {
      "type": "composer",
      "url": "https://internal-modules-repo.oxid-esales.com/"
    },
    {
      "type": "composer",
      "url": "https://enterprise-edition.packages.oxid-esales.com/"
    }
  ],
  "require": {
    "oxid-esales/oxideshop-metapackage-ee": "v6.2.5",
    "oxid-professional-services/paypal": "^0.0.2",
    "oxid-professional-services/paypal-client": "^0.0.1",
    "oxid-professional-services/paypallocale": "^0.0.1"
  },
  "replace": {
        "oxid-professional-services/usercentrics": "*",
        "oxid-esales/paymorrow-module": "*",
        "oxid-esales/paypal-module": "*",
        "ddoe/visualcms-module": "*",
        "ddoe/wysiwyg-editor-module": "*",
        "payone-gmbh/oxid-6": "*",
        "topconcepts/oxid-klarna-6": "*",
        "amzn/amazon-pay-sdk-php": "*",
        "bestit/amazonpay4oxid": "*"
  },
  "require-dev": {
    "oxid-esales/testing-library": "^v7.3.0",
    "incenteev/composer-parameter-handler": "^v2.0.0",
    "oxid-esales/oxideshop-ide-helper": "^v4.0.0",
    "squizlabs/php_codesniffer": "3.*"
  },
  "scripts": {
    "post-install-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "@oe:ide-helper:generate"
    ],
    "post-update-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "@oe:ide-helper:generate"
    ],
    "oe:ide-helper:generate": [
      "if [ -f ./vendor/bin/oe-eshop-ide_helper ]; then oe-eshop-ide_helper; fi"
    ],
    "repositories": {
      "0": {
        "type": "composer",
        "url": "https://internal-modules-repo.oxid-esales.com/"
      },
      "1": {
        "type": "composer",
        "url": "https://enterprise-edition.packages.oxid-esales.com/"
      }
    }
  },
  "config": {
    "sort-packages": true,
    "gitlab-domains": [
      "psgit.oxid-esales.com",
      "internal-modules-repo.oxid-esales.com"
    ],
    "preferred-install": {
      "*": "dist"
    }
  },
  "extra": {
    "incenteev-parameters": {
      "file": "test_config.yml",
      "dist-file": "vendor/oxid-esales/testing-library/test_config.yml.dist",
      "parameter-key": "mandatory_parameters",
      "env-map": {
        "shop_path": "SHOP_PATH",
        "shop_tests_path": "SHOP_TESTS_PATH",
        "partial_module_paths": "PARTIAL_MODULE_PATHS"
      }
    }
  }
}
